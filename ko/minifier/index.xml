      <description>
        
        
        &lt;link rel=&#34;stylesheet&#34; href=&#34;https://toolkit.devkuma.com/content/minifier/minifier.css&#34; /&gt;
&lt;div class=&#34;mb-3 col-sm-12&#34;&gt;
    &lt;label for=&#34;css&#34; clas=&#34;h3&#34;&gt;SQL Content&lt;/label&gt;
    &lt;div id=&#34;editor&#34; style=&#34;width:100%;height:400px;&#34;&gt;select col
from abc
where col = 1&lt;/div&gt;
