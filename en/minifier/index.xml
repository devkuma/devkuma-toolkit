&lt;div class=&#34;mb-3 col-sm-12&#34;&gt;
    &lt;label for=&#34;css&#34; clas=&#34;h3&#34;&gt;JSON Content&lt;/label&gt;
    &lt;div id=&#34;editor&#34; style=&#34;width:100%;height:400px;&#34;&gt;{}&lt;/div&gt;
